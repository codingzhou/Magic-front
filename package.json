{
  "name": "magic-front",
  "version": "1.3.14",
  "description": "micro frontend",
  "keywords": [
    "micro frontend",
    "microfrontend",
    "micro frontends"
  ],
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "concurrently \"yarn start:main\" \"yarn start:performance\" \"yarn start:appManagement\"",
    "install:apps": "yarn && npm run install:main && npm run install:performance && npm run install:appManagement",
    "install:main": "cd applications/main && yarn",
    "install:performance": "cd applications/performance && yarn",
    "install:appManagement": "cd applications/appManagement && yarn",
    "build:apps": "npm run build:main && npm run build:performance && npm run build:appManagement",
    "build:main": "cd applications/main && yarn build",
    "build:performance": "cd applications/performance && yarn build",
    "build:appManagement": "cd applications/appManagement && yarn build",
    "start:main": "cd applications/main && yarn start",
    "start:performance": "cd applications/performance && yarn start",
    "start:appManagement": "cd applications/appManagement && yarn start",
    "build": "father-build",
    "release": "np --no-cleanup --yolo --no-publish",
    "prepublishOnly": "yarn lint && yarn build",
    "lint": "#yarn lint:js && yarn lint:prettier",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:fix": "# yarn lint:js -- --fix",
    "lint:prettier": "check-prettier lint",
    "prettier": "prettier -c --write **/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yzw7489757/Magic-front.git"
  },
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "author": "yzw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yzw7489757/Magic-front/issues"
  },
  "homepage": "https://github.com/yzw7489757/Magic-fronts#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "import-html-entry": "^1.3.9",
    "lodash": "^4.17.11",
    "mobile-detect": "^1.4.4",
    "single-spa": "^4.3.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.129",
    "@types/vfile-message": "1.x",
    "@umijs/fabric": "^1.1.10",
    "babel-plugin-import": "^1.12.1",
    "check-prettier": "^1.0.3",
    "concurrently": "^4.1.2",
    "father-build": "^1.7.0",
    "husky": "^2.3.0",
    "lint-staged": "^9.4.2",
    "np": "^5.0.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json,css,md}": [
      "yarn prettier",
      "git add"
    ],
    "**/*.{js,ts}": [
      "yarn lint:fix",
      "git add"
    ]
  }
}
